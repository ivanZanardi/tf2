# Set the package version
set(pycomet_VERSION "@PROJECT_VERSION@")

# Set the package configuration variables
set(pycomet_FOUND 1)
set(pycomet_INCLUDE_DIR "@CMAKE_INSTALL_PREFIX@/include")
set(pycomet_INCLUDE_DIRS ${pycomet_INCLUDE_DIR})
set(pycomet_LIBRARY_DIR "@CMAKE_INSTALL_PREFIX@/lib@LIB_SUFFIX@")

# Find the library using the full path or HINTS
find_library(pycomet_LIBRARY NAMES pycomet HINTS ${pycomet_LIBRARY_DIR})

# Set the LIBRARIES variable
set(pycomet_LIBRARIES ${pycomet_LIBRARY})
